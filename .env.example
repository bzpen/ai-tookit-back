# ===== 应用基础配置 =====
NODE_ENV=development
PORT=3000

# ===== CORS配置 =====
CORS_ORIGIN=http://localhost:3000

# ===== 数据库配置 - Supabase =====
SUPABASE_URL=https://www.supabase.co
SUPABASE_ANON_KEY=SUPABASE_ANON_KEY

# ===== JWT认证配置 =====
JWT_SECRET=your_jwt_secret_key_minimum_32_characters
JWT_EXPIRES_IN=7d

# ===== 安全配置 =====
BCRYPT_ROUNDS=12

# ===== 限流配置 =====
RATE_LIMIT_WINDOW=15
RATE_LIMIT_MAX=100

# ===== 日志配置 =====
LOG_LEVEL=info
