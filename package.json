{
  "name": "ai-toolkit-back",
  "version": "1.0.0",
  "description": "AI工具集后端服务",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only -r module-alias/register src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "db:init": "ts-node scripts/init-database.ts init",
    "db:cleanup": "ts-node scripts/init-database.ts cleanup",
    "deploy:prepare": "node -e \"console.log('使用 .\\\\scripts\\\\deploy.ps1 或 ./scripts/deploy.sh 进行部署准备')\"",
    "deploy:test": "npm run build && npm run test",
    "deploy:docker": "docker build -t test-back . --target production",
    "deploy:render": "echo 'Push to GitHub to trigger Render deployment'"
  },
  "keywords": [
    "ai",
    "replicate",
    "backend",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@supabase/supabase-js": "^2.50.3",
    "axios": "^1.6.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cron": "^3.1.6",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-session": "^1.18.1",
    "google-auth-library": "^10.1.0",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "uuid": "^11.1.0",
    "validator": "^13.15.15",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.2",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.9.0",
    "@types/passport": "^1.0.17",
    "@types/passport-google-oauth20": "^2.0.16",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "@types/validator": "^13.15.2",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "supertest": "^7.1.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2"
  },
  "packageManager": "pnpm@9.9.0",
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0"
  },
  "_moduleAliases": {
    "@": "dist",
    "@/controllers": "dist/controllers",
    "@/services": "dist/services",
    "@/models": "dist/models",
    "@/middleware": "dist/middleware",
    "@/routes": "dist/routes",
    "@/utils": "dist/utils",
    "@/config": "dist/config",
    "@/jobs": "dist/jobs",
    "@/types": "dist/types"
  }
}
